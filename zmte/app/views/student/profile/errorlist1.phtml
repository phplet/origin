<script type="text/javascript">
    $(function(){
        /*顶部导航JS效果*/
        var nav = $(".cls_info_header_list li a");
        nav.each(function(){
            $(this).mouseover(function(){
                $(this).addClass("cls_current").siblings().removeClass("cls_current");
            });
            $(this).mouseleave(function(){
                $(this).removeClass("cls_current");
            });
            $(this).click(function(){
                window.location.href = $(this).children("a").attr("href");
            });
        });
        var url = window.location.href;
        if (url.indexOf("profile") > 0){
            nav.eq(0).addClass("cls_student_current").siblings().removeClass("cls_student_current");
        }
        if (url.indexOf("onlineclasslist") > 0){
            nav.eq(2).addClass("cls_student_current").siblings().removeClass("cls_student_current");
        }
        if (url.indexOf("selcourseknowledge") > 0
           || url.indexOf("do") > 0){
            nav.eq(1).addClass("cls_student_current").siblings().removeClass("cls_student_current");
        }
        if (url.indexOf("myerrorstatlist") > 0
           || url.indexOf("exercise/view") > 0){
            nav.eq(3).addClass("cls_student_current").siblings().removeClass("cls_student_current");
        }
        if (url.indexOf("wallet") > 0){
            nav.eq(4).addClass("cls_student_current").siblings().removeClass("cls_student_current");
        }
        if (url.indexOf("mytestworklist") > 0){
            nav.eq(5).addClass("cls_student_current").siblings().removeClass("cls_student_current");
        }
    });

