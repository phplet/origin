                if (pos1 >= 0){
                    return (function(){
                       ["person." +nav+"()"];
                    }());
                } else{
                    if (nav == 'index'){
                        var param = {
                            'name': "当前登录用户姓名在这里"
                        };
                        param.has_course = 0;
                        param.has_testwork = 0;
                        person.index(param);
                        setInterval(function(){
                            person.index(param);
                        },3000);
                    }else if (nav == 'mytestworklist') {
                        person.mytestworklist(0);
                    } else if (nav == 'onlineclasslist'){
                        person.onlineclasslist(0);
                    }
                }
            } 
        });
